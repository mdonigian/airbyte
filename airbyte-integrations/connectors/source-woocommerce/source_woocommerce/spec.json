{
  "documentationUrl": "https://docsurl.com",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Woocommerce Spec",
    "type": "object",
    "required": ["url", "consumer_key", "consumer_secret"],
    "additionalProperties": false,
    "properties": {
      "url": {
        "type": "string",
        "description": "Your Store URL, example: http://woo.dev"
      },
      "consumer_key": {
        "type": "string",
        "description": "Your API consumer key"
      },
      "consumer_secret": {
        "type": "string",
        "description": "Your API consumer secret"
      }
    }
  }
}
